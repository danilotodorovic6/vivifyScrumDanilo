        "taskName": "textarea[name='item_name']",
        "saveTask": "button[name='new_item_save']",
        "cancelTask": "button[name='new_item_cancel']",
        "createdTask": ".vs-u-padding--sm",
        "taskTitle": ".vs-c-task__title .vue-simple-markdown",
        "saveTitle": "button[name='item_name_save']",
        "cancelTitle": "button[name='item_name_cancel']",
        "description": "button[name='item_description_edit']",
        "descriptionText": "textarea[name='description']",
        "saveDescription": "button[name='item_description_save']",
        "cancelDescription": "button[name='item_description_cancel']",
        "comment": "textarea[name='new_comment']",
        "saveComment": "button[name='comment_save']",
        "deleteComment": "button[name='delete_comment']",
        "changeSprint": "div[name='sprint-info-dropdown']",
        "backlog": ".vs-c-dropdown li:nth-of-type(1)",
        "taskOptions": "div[title='Options']",
        "deleteOption": "li[name='delete-option']"
    }
