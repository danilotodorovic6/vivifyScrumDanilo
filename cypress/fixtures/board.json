        "chooseOrganizationButton": ".el-input__inner",
        "chooseOrganizationList": ".el-select-dropdown__item",
        "boardTitle": "input[name='name']",
        "boardType": {
            "scrum": "[name='type_scrum']",
            "kanban": "[name='type_kanban']"
        },
        "uploadBoardLogo": ".el-upload--text",
        "closeButton": "button[name='close-new-board-modal-btn']",
        "nextButton": "button[name='next_btn']",
        "backButton": "button[name='prev_btn']",
        "boardDeletedOKButton": ".vs-c-modal--features-button > .vs-c-btn"
    },
    "boardInfo": {
        "boardDetails": {
            "boardTitleInput": "input[name='name']",
            "boardCodeInput": "input[name='code']",
            "boardDescriptionInput": "textarea[name='description']",
            "updateBoardInfoButton": ".vs-u-text--left > button[type='submit']",
            "changingOrganiation": ".vs-c-dropdown-underline input[type='text']",
            "selectingOrganization": ".vs-c-board-dropdown-item",
            "changingBoardType": ".el-icon-caret-bottom",
            "boardTypes": {
                "scrumType": ".el-dropdown-menu li:nth-of-type(1)",    
                "kanbanType": ".el-dropdown-menu li:nth-of-type(2)"        
            },
            "uploadLogo": ".vs-c-img--board-avatar-lg",
            "archiveBoard": ".vs-c-btn--success",
            "deleteBoard": {
                "deleteBoardClick": ".vs-c-btn--warning",
                "saveButton": "button[name='save-btn']",
                "cancelButton": "button[name='cancel-btn']"
                }
        }
    },
