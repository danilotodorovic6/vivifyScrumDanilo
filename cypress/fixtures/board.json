{
    "boardSidebar": {
        "productBacklog": "li:nth-of-type(1) .vs-c-site-logo",
        "stats": "li:nth-of-type(2) .vs-c-site-logo",
        "history": "li:nth-of-type(3) .vs-c-site-logo",
        "labels": "li:nth-of-type(4) .vs-c-site-logo",
        "files": "li:nth-of-type(5) .vs-c-site-logo",
        "documents": "li:nth-of-type(6) .vs-c-site-logo",
        "worklogs": "li:nth-of-type(7) .vs-c-site-logo",
        "teamMembers": "li:nth-of-type(8) .vs-c-site-logo",
        "trash": "li:nth-of-type(9) .vs-c-site-logo",
        "configuration": "li:nth-of-type(10) .vs-c-site-logo"
    },
    "newBoard": {
